<?xml version="1.0" encoding="UTF-8"?>
<manifest>

  <!-- GitHub remote -->
  <remote
      name="github"
      fetch="https://github.com"
      revision="main" />

  <!-- Device tree for Motorola E7 Plus guam -->
  <project
      path="device/motorola/guam"
      name="shipukacapri/android_device_motorola_guam"
      remote="github"
      revision="infinity" />

  <!-- Common device tree for sm6225 -->
  <project
      path="device/motorola/sm6225-common"
      name="LineageOS/android_device_motorola_sm6225-common"
      remote="github"
      revision="lineage-23.1" />

  <!-- Kernel -->
  <project
      path="kernel/motorola/sm6225"
      name="LineageOS/android_kernel_motorola_sm6225"
      remote="github"
      revision="lineage-23.1" />

  <!-- Vendor -->
  <project
      path="vendor/motorola/guam"
      name="TheMuppets/proprietary_vendor_motorola_guam"
      remote="github"
      revision="lineage-23.1" />

  <project
      path="vendor/motorola/sm6225-common"
      name="TheMuppets/proprietary_vendor_motorola_sm6225-common"
      remote="github"
      revision="lineage-23.1" />

  <!-- Motorola hardware -->
  <project
      name="LineageOS/android_hardware_motorola"
      path="hardware/motorola"
      remote="github"
      revision="lineage-23.1" />

  <!-- QCOM SEPolicy -->
  <project
      name="LineageOS/android_device_qcom_sepolicy_vndr"
      path="device/qcom/sepolicy_vndr"
      remote="github"
      revision="lineage-21.0-legacy-um" />

  <project
      name="LineageOS/android_device_qcom_sepolicy"
      path="hardware/qcom/sepolicy"
      remote="github"
      revision="lineage-23.1" />

</manifest>
